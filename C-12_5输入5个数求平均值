#include<stdio.h>
int main()
{
    int a,b,c,d,e;
    scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
    double Average = (a + b + c + d + e) / (double)5;
    printf("%.1f\n",Average);
    return 0;
}





//用数组求
#include<stdio.h>
int main()
{
	int a[5];
	int i;
	double Average = 0;
	for (i = 0; i <= 4;i++)
	{
		scanf_s("%d", &a[i]);
		Average = Average + a[i] / 5.0;
	}
	printf("%.1lf\n", Average);
	return 0;
}
