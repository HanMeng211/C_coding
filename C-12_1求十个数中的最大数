#include<stdio.h>
int main()
{
	int arr[10];
	int i;
	int max;
	for (i = 0; i < 10; i++)   //输入十个数
	{
		scanf_s("%d", &arr[i]);
	}
	max = arr[0];
	for (i = 1; i < 10; i++)   //判断a[i]和max大小如果a[i]>max就赋值给max然后继续循环比较
	{
		if (arr[i] > max)
		{
			max = arr[i];
		}
	}
	printf("max=%d\n", max);
	return 0;
}
