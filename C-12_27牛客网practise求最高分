#include<stdio.h>
int GetMax(int a,int b,int c)
{
    if(a >= b && a >= c)
    {
        return a;
    }
    else if(b >= a && b >= c)
    {
        return b;
    }
    else
    {
        return c;
    }
}
int main()
{
    int a,b,c;
    while(scanf("%d %d %d",&a,&b,&c) != EOF)
    {
        printf("%d\n",GetMax(a,b,c));
    }
    return 0;
}
