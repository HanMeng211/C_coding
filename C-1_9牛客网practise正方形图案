#include<stdio.h>
int main()
{
    int Side;
        while(scanf("%d",&Side) != EOF)
        {
            for(int i = 0;i < Side;i++)//打印行
            {
                for(int i = 0;i < Side;i++)//打印列
                {
                    printf("* ");
                }
                printf("\n");
            }
        }
    return 0;
}
