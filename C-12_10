#include<stdio.h>
int CountBitNum(unsigned int x)
{
	int count = 0;
	for (int i = 1; i <= 32; i++)
	{
		if ((x & 1) == 1)
	{
		count += 1;
	}
		x >>= 1;
	}
	return count;
}
int main()
{
	printf("%d\n", CountBitNum(0xFFFFFFFF));
	printf("%d\n", CountBitNum(1));
	printf("%d\n", CountBitNum(123));
	return 0;
}
