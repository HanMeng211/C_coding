#include<stdio.h>
#include<string.h>
int main()
{
    char password1[100],password2[100];
    //输入的参数是已经定义好的“字符串数组”，不用加&，因为在C语言中数组名就代表该数组的起始地址
    scanf("%s  %s",password1,password2);
    if(strcmp(password1,password2) == 0)
    {
        printf("same\n");
    }
    else
    {
        printf("different\n");
    }
    
    return 0;
}
