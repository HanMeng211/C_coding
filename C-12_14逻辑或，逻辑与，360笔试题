//逻辑或   有真为真
#include<stdio.h>
int main()
{
	int i = 0, a = 0, b = 2, c = 3, d = 4;
	i = a++ || ++b || d++;
	printf("i=%d\n a=%d\n b=%d\n c=%d\n d=%d\n",i, a, b, c, d);
	return 0;
}





//逻辑与    有假为假
#include<stdio.h>
int main()
{
	int i = 0, a = 0, b = 2, c = 3, d = 4;
	i = a++ && ++b && d++;
	printf("i=%d\n a=%d\n b=%d\n c=%d\n d=%d\n",i, a, b, c, d);
	return 0;
}
