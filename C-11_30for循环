//for循环和break
#include<stdio.h>
int main()
{
	for (int i = 1; i <= 10; i++)
	{
		printf("%d\n", i);
	}
	return 0;
}







//for循环和continue
#include<stdio.h>
int main()
{
	int i;
	for (i = 1; i <= 10; i++)
	{
		if (i == 5)
		{
			continue;
		}
		printf("%d\n", i);
	}
	return 0;
}








#include<stdio.h>
int main()
{
	int x = 0, y = 0;
	for (x = 0, y = 0; x < 2 , y < 5; ++x, y++)  //逗号表达式整个返回值取逗号以后
	{
		printf("hehe\n");
	}
	return 0;
}
	
